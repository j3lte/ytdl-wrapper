{
  "name": "@j3lte/ytdlp-wrapper",
  "version": "0.1.0",
  "exports": {
    ".": "./src/mod.ts"
  },
  "tasks": {
    "test": "deno test -A",
    "prep": "deno fmt && deno lint",
    "publish:test": "deno publish --dry-run --allow-dirty"
  },
  "fmt": {
    "exclude": [
      "**/*.md"
    ],
    "lineWidth": 120,
    "indentWidth": 2
  },
  "imports": {
    "@denosaurs/event": "jsr:@denosaurs/event@^2.0.2",
    "@gnome/exec": "jsr:@gnome/exec@^0.6.0",
    "@gnome/runtime-info": "jsr:@gnome/runtime-info@^0.1.0",
    "@std/assert": "jsr:@std/assert@^1.0.4",
    "@std/testing": "jsr:@std/testing@^1.0.2"
  },
  "publish": {
    "include": [
      "src/mod.ts",
      "src/types.ts",
      "src/utils.ts",
      "src/wrapper.ts",
      "README.md",
      "LICENSE",
      "deno.json"
    ]
  }
}
